{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "react-app",
        "react-app/jest",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "ecmaFeatures": {
            "jsx": true
          },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [
            "error",
            "double",
            {
                "avoidEscape": true
            }
        ],
        "react/jsx-uses-react": ["off"],
        "react/react-in-jsx-scope": ["off"],
        "react/react-props-no-spreading": ["warn"],
        "no-shadow": "off"
    }
}
