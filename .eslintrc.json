{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "plugin:react/recommended",
      "react-app",
      "react-app/jest",
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "react/jsx-uses-react": ["off"],
      "react/react-in-jsx-scope": ["off"],
      "no-shadow": "off",
      "react/jsx-filename-extension": [0]
    }
  }